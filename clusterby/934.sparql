
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P4501 ?l1 ;
		wdt:P2244 ?x1 ;
		wdt:P2213 ?a1 ;
		wdt:P2045 ?h1 ;
		wdt:P1096 ?b1 ;
		wdt:P2325 ?c1 ;
		wdt:P2233 ?k1 ;
		wdt:P2248 ?r1 .

}
CLUSTER BY ?l1 ?x1 ?a1 ?h1 ?b1 ?c1 ?k1 ?r1 WITH %s AS ?c
