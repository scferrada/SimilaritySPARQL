
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P1457 ?e1 ;
		wdt:P2248 ?q1 ;
		wdt:P2243 ?i1 ;
		wdt:P2213 ?v1 ;
		wdt:P2045 ?b1 ;
		wdt:P1096 ?w1 ;
		wdt:P2325 ?x1 ;
		wdt:P4501 ?r1 .

}
CLUSTER BY ?e1 ?q1 ?i1 ?v1 ?b1 ?w1 ?x1 ?r1 WITH %s AS ?c
