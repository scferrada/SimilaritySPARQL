
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P1457 ?p1 ;
		wdt:P2248 ?d1 ;
		wdt:P2244 ?v1 ;
		wdt:P2213 ?z1 ;
		wdt:P2045 ?o1 ;
		wdt:P2325 ?r1 ;
		wdt:P4501 ?b1 .

}
CLUSTER BY ?q1 ?p1 ?d1 ?v1 ?z1 ?o1 ?r1 ?b1 WITH %s AS ?c
