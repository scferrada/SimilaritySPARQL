
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P1457 ?c1 ;
		wdt:P2248 ?x1 ;
		wdt:P2243 ?g1 ;
		wdt:P2213 ?o1 ;
		wdt:P2045 ?f1 ;
		wdt:P1096 ?u1 ;
		wdt:P2233 ?s1 ;
		wdt:P4501 ?w1 .

}
CLUSTER BY ?c1 ?x1 ?g1 ?o1 ?f1 ?u1 ?s1 ?w1 WITH %s AS ?c
