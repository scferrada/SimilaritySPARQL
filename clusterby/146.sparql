
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P2146 ?h1 ;
		wdt:P1457 ?q1 ;
		wdt:P2248 ?p1 ;
		wdt:P2244 ?d1 ;
		wdt:P2213 ?e1 ;
		wdt:P1096 ?i1 ;
		wdt:P2325 ?a1 ;
		wdt:P4501 ?l1 .

}
CLUSTER BY ?h1 ?q1 ?p1 ?d1 ?e1 ?i1 ?a1 ?l1 WITH %s AS ?c
