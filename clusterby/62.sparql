
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P4501 ?y1 ;
		wdt:P2244 ?s1 ;
		wdt:P2243 ?k1 ;
		wdt:P2213 ?z1 ;
		wdt:P2325 ?o1 ;
		wdt:P2233 ?a1 ;
		wdt:P2248 ?n1 .

}
CLUSTER BY ?c1 ?y1 ?s1 ?k1 ?z1 ?o1 ?a1 ?n1 WITH %s AS ?c
