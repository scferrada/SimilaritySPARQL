
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P2146 ?u1 ;
		wdt:P1457 ?c1 ;
		wdt:P2248 ?o1 ;
		wdt:P2244 ?p1 ;
		wdt:P2243 ?e1 ;
		wdt:P2045 ?g1 ;
		wdt:P1096 ?w1 ;
		wdt:P2325 ?x1 .

}
CLUSTER BY ?u1 ?c1 ?o1 ?p1 ?e1 ?g1 ?w1 ?x1 WITH %s AS ?c
