
PREFIX sim:<http://sj.dcc.uchile.cl/sim#>
PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT * WHERE {
	?id1 wdt:P1457 ?w1 ;
		wdt:P2248 ?t1 ;
		wdt:P2244 ?e1 ;
		wdt:P2243 ?s1 ;
		wdt:P2213 ?p1 ;
		wdt:P1096 ?c1 ;
		wdt:P2325 ?m1 ;
		wdt:P4501 ?h1 .

}
CLUSTER BY ?w1 ?t1 ?e1 ?s1 ?p1 ?c1 ?m1 ?h1 WITH %s AS ?c
